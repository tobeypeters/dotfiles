{
    "editor.minimap.enabled": false,
    "breadcrumbs.enabled": false,
    "python.pythonPath": "/usr/bin/python3.8",
    "window.menuBarVisibility": "toggle",
    "workbench.colorTheme": "Wal",
    "editor.largeFileOptimizations": false,
    "editor.columnSelection": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "kite.showWelcomeNotificationOnStartup": false,
    "python.languageServer": "Pylance",
    "editor.fontLigatures": true,
    // The Cursive font is operator Mono, it's $200 and you need to buy it to get the cursive
    "editor.fontFamily": "'System San Francisco Display', 'cascadia code', Operator Mono, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 13,
    "editor.lineHeight": 18,
    "editor.letterSpacing": 0.5,
    "files.trimTrailingWhitespace": true,
    "editor.fontWeight": "400",
    "prettier.eslintIntegration": true,
    "editor.cursorWidth": 5,
    "editor.renderWhitespace": "all",
    "editor.cursorBlinking": "solid",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.activityBar.location": "hidden",
    "workbench.editor.empty.hint": "hidden",
    "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
    "liveServer.settings.donotShowInfoMsg": true,
    "window.zoomLevel": 2,
    "git.openRepositoryInParentFolders": "never",
    "cmake.showOptionsMovedNotification": false,
    "editor.guides.bracketPairs": true,
    "editor.stickyScroll.enabled": false,
}